	"C_Cpp.autocompleteAddParentheses": true,
	"C_Cpp.clang_format_fallbackStyle": "{ BasedOnStyle: Google, IndentWidth: 4, TabWidth: 4, UseTab: Always, AlignTrailingComments: true, AlignConsecutiveAssignments: AcrossComments, IndentPPDirectives: BeforeHash }",
	"chat.editor.fontFamily": "JetBrainsMonoNL Nerd Font, iA Writer Mono V, GeistMono Nerd Font Propo, JetBrainsMono Nerd Font",
	"debug.console.fontFamily": "JetBrainsMonoNL Nerd Font, iA Writer Mono V, GeistMono Nerd Font Propo, JetBrainsMono Nerd Font",
	"editor.accessibilitySupport": "off",
	"editor.autoClosingBrackets": "beforeWhitespace",
	"editor.codeLensFontFamily": "JetBrainsMonoNL Nerd Font, iA Writer Mono V, GeistMono Nerd Font Propo, JetBrainsMono Nerd Font",
	"editor.cursorBlinking": "solid",
	"editor.cursorStyle": "block",
	"editor.cursorSurroundingLines": 8,
	"editor.fontFamily": "BerkeleyMonoTrial Nerd Font",
	// "editor.fontFamily": "JetBrainsMonoNL Nerd Font",
	// "editor.fontFamily": "Monaspace Neon",
	// "editor.fontFamily": "iA Writer Mono V",
	// "editor.fontFamily": "GeistMono Nerd Font Propo",
	// "editor.fontFamily": "Monaco",
	"editor.fontSize": 14.5,
	"editor.formatOnSave": true,
	"editor.glyphMargin": true,
	"editor.guides.bracketPairs": true,
	"editor.guides.bracketPairsHorizontal": false,
	"editor.inlayHints.fontFamily": "JetBrainsMonoNL Nerd Font, iA Writer Mono V, GeistMono Nerd Font Propo, JetBrainsMono Nerd Font",
	"editor.insertSpaces": false,
	"editor.lineNumbers": "relative",
	"editor.matchBrackets": "near",
	"editor.mouseWheelScrollSensitivity": 0.5,
	"editor.renderControlCharacters": true,
	"editor.scrollbar.horizontal": "auto",
	"editor.scrollbar.vertical": "auto",
	"editor.tabSize": 4,
	"editor.wordBasedSuggestions": "matchingDocuments",
	"editor.wordWrap": "off",
	"editor.wrappingIndent": "none",
	"explorer.confirmDragAndDrop": false,
	"extensions.experimental.affinity": {
		"vscodevim.vim": 1
	},
	"files.autoSave": "afterDelay",
	"files.exclude": {
		"**/.git": true,
		"**/.svn": true,
		"**/.hg": true,
		"**/CVS": true,
		"**/.DS_Store": true,
		"**/Thumbs.db": true,
		"**/venv": true,
	},
	"files.trimTrailingWhitespace": true,
	"git.autofetch": true,
	"github.copilot.enable": {
		"*": false,
		"plaintext": false,
		"markdown": false,
		"scminput": false,
		"python": false,
		"java": false
	},
	"go.formatTool": "gofmt",
	"go.toolsManagement.autoUpdate": true,
	"go.useLanguageServer": true,
	"haskell.manageHLS": "GHCup",
	// "markdown.preview.fontFamily": "iA Writer Quattro V, JetBrainsMonoNL Nerd Font, iA Writer Mono V, GeistMono Nerd Font Propo, JetBrainsMono Nerd Font",
	"notebook.output.fontFamily": "JetBrainsMonoNL Nerd Font, iA Writer Mono V, GeistMono Nerd Font Propo, JetBrainsMono Nerd Font",
	"python.defaultInterpreterPath": "/opt/homebrew/bin/python3",
	"python.terminal.activateEnvironment": true,
	"[python]": {
		"editor.insertSpaces": true,
		"editor.defaultFormatter": "ms-python.black-formatter",
		"editor.tabSize": 4
	},
	"scm.defaultViewMode": "tree",
	"scm.inputFontFamily": "JetBrainsMonoNL Nerd Font, iA Writer Mono V, GeistMono Nerd Font Propo, JetBrainsMono Nerd Font",
	"security.workspace.trust.untrustedFiles": "open",
	"shellcheck.executablePath": "/opt/homebrew/bin/shellcheck",
	"terminal.external.osxExec": "Ghostty.app",
	"terminal.integrated.fontFamily": "JetBrainsMonoNL Nerd Font, MesloLGS NF, Monaco",
	"terminal.integrated.fontSize": 14,
	"terminal.integrated.initialHint": false,
	"workbench.colorCustomizations": {
		"editorError.foreground": "#FFFFFF50",
		"editorInfo.foreground": "#FFFFFF50",
		"editorWarning.foreground": "#FFFFFF50"
	},
	"workbench.colorTheme": "Ayu Mirage Bordered",
	"workbench.editor.empty.hint": "hidden",
	"workbench.list.mouseWheelScrollSensitivity": 0.5,
	"vim.cursorStylePerMode.insert": "line",
	"vim.cursorStylePerMode.normal": "block",
	"vim.cursorStylePerMode.replace": "line",
	"vim.cursorStylePerMode.visual": "line",
	"vim.cursorStylePerMode.visualblock": "line",
	"vim.cursorStylePerMode.visualline": "block",
	"vim.foldfix": true,
	"vim.hlsearch": true,
	"vim.highlightedyank.enable": true,
	"vim.leader": "<space>",
	"vim.normalModeKeyBindings": [
		{
			"before": [
				"[",
				"d"
			],
			"commands": [
				"editor.action.marker.prev"
			]
		},
		{
			"before": [
				"]",
				"d"
			],
			"commands": [
				"editor.action.marker.next"
			]
		},
		{
			"before": [
				"<leader>",
				"f",
				"f"
			],
			"commands": [
				"actions.find"
			]
		},
		{
			"before": [
				"<leader>",
				"f",
				"r"
			],
			"commands": [
				"editor.action.startFindReplaceAction"
			]
		},
		{
			"before": [
				"<leader>",
				"c",
				"a"
			],
			"commands": [
				"editor.action.quickFix"
			]
		},
		{
			"before": [
				"<leader>",
				"o"
			],
			"commands": [
				"workbench.action.quickOpen"
			]
		},
		{
			"before": [
				"<leader>",
				"f",
				"d"
			],
			"commands": [
				"editor.action.formatDocument"
			]
		},
		{
			"before": [
				"<leader>",
				"t"
			],
			"commands": [
				"workbench.action.terminal.toggleTerminal"
			]
		},
		{
			"before": [
				"<leader>",
				"v",
				"r",
				"n"
			],
			"commands": [
				"editor.action.rename"
			]
		},
		{
			"before": [
				"g",
				"d"
			],
			"commands": [
				"editor.action.peekDefinition"
			]
		},
		{
			"before": [
				"g",
				"r"
			],
			"commands": [
				"editor.action.referenceSearch.trigger"
			]
		},
		{
			"before": [
				"K"
			],
			"commands": [
				"editor.action.showHover"
			]
		},
		{
			"before": [
				"<leader>",
				"<tab>"
			],
			"commands": [
				"workbench.action.nextEditor"
			]
		},
		{
			"before": [
				"<leader>",
				"<shift+tab>"
			],
			"commands": [
				"workbench.action.previousEditor"
			]
		}
	],
	"vim.visualModeKeyBindings": [
		{
			"before": [
				"J"
			],
			"commands": [
				"editor.action.moveLinesDownAction"
			]
		},
		{
			"before": [
				"K"
			],
			"commands": [
				"editor.action.moveLinesUpAction"
			]
		},
		{
			"before": [
				"<"
			],
			"after": [
				"<",
				"g",
				"v"
			]
		},
		{
			"before": [
				">"
			],
			"after": [
				">",
				"g",
				"v"
			]
		}
	],
	"redhat.telemetry.enabled": false,
	"editor.minimap.enabled": false,
	"diffEditor.ignoreTrimWhitespace": false,
	"cmake.showOptionsMovedNotification": false,
	"cmake.pinnedCommands": [
		"workbench.action.tasks.configureTaskRunner",
		"workbench.action.tasks.runTask"
	],
	"cmake.configureOnOpen": true,
	"python.analysis.autoImportCompletions": true,
	"python.analysis.typeCheckingMode": "off",
	"python.analysis.autoFormatStrings": true,
	"editor.bracketPairColorization.enabled": false,
	"scm.diffDecorations": "gutter",
	"editor.guides.highlightActiveIndentation": "always",
	"todo-tree.highlights.defaultHighlight": {
		"background": "#00000000",
	},
	"gitlens.codeLens.enabled": false,
	"gitlens.currentLine.enabled": false,
	"python.analysis.completeFunctionParens": true,
	"github.copilot.editor.enableAutoCompletions": true,
	"python.languageServer": "Pylance",
	"editor.defaultFormatter": "ms-python.black-formatter",
	"python.analysis.diagnosticSeverityOverrides": {
		"reportUnusedImport": "warning",
		"reportUnusedClass": "warning",
		"reportUnusedVariable": "warning",
		"reportUnusedFunction": "warning",
		"reportUnusedExpression": "warning",
	},
	"editor.lineHeight": 1.6,
	"[cpp]": {
		"editor.defaultFormatter": "ms-vscode.cpptools"
	},
	"[csv]": {
		"editor.wordWrap": "on"
	},
	"[markdown]": {
		"editor.wordWrap": "on"
	},
	"terminal.integrated.enableVisualBell": true,
	"accessibility.signals.terminalBell": {
		"sound": "on"
	},
	"workbench.activityBar.location": "bottom",
	// "workbench.editor.editorActionsLocation": "hidden",
	"breadcrumbs.enabled": false,
	"markdown.preview.scrollEditorWithPreview": false,
	"markdown.preview.scrollPreviewWithEditor": false,
	"vsicons.dontShowNewVersionMessage": true,
	"C_Cpp.default.cppStandard": "c++20",
}