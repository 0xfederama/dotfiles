{
	"C_Cpp.autocompleteAddParentheses": true,
	"C_Cpp.clang_format_fallbackStyle": "{ BasedOnStyle: Google, IndentWidth: 4, TabWidth: 4, UseTab: Always, AlignTrailingComments: true, AlignConsecutiveAssignments: AcrossComments, IndentPPDirectives: BeforeHash }",
	"editor.autoClosingBrackets": "beforeWhitespace",
	"editor.bracketPairColorization.enabled": false,
	"editor.cursorBlinking": "solid",
	"editor.cursorStyle": "block",
	"editor.cursorSurroundingLines": 8,
	"editor.fontFamily": "JetBrainsMono Nerd Font, Monaco",
	"editor.fontSize": 14.5,
	"editor.formatOnSave": true,
	"editor.guides.bracketPairs": true,
	"editor.guides.bracketPairsHorizontal": false,
	"editor.insertSpaces": false,
	"editor.lineNumbers": "relative",
	"editor.matchBrackets": "near",
	"editor.mouseWheelScrollSensitivity": 0.5,
	"editor.renderControlCharacters": true,
	"editor.tabSize": 4,
	"editor.wordBasedSuggestions": true,
	"explorer.confirmDragAndDrop": false,
	"extensions.experimental.affinity": {
		"vscodevim.vim": 1
	},
	"files.autoSave": "afterDelay",
	"files.trimTrailingWhitespace": true,
	"git.autofetch": true,
	"github.copilot.enable": {
		"*": true,
		"markdown": false,
		"plaintext": false,
		"scminput": false
	},
	"go.formatTool": "gofmt",
	"go.toolsManagement.autoUpdate": true,
	"go.useLanguageServer": true,
	"haskell.manageHLS": "GHCup",
	"python.defaultInterpreterPath": "python3",
	"scm.defaultViewMode": "tree",
	"terminal.external.osxExec": "iTerm.app",
	"terminal.integrated.fontFamily": "JetBrainsMono Nerd Font, MesloLGS NF, Monaco",
	"terminal.integrated.fontSize": 14,
	"vim.cursorStylePerMode.insert": "line",
	"vim.cursorStylePerMode.normal": "block",
	"vim.cursorStylePerMode.replace": "block",
	"vim.cursorStylePerMode.visual": "block",
	"vim.cursorStylePerMode.visualblock": "block",
	"vim.cursorStylePerMode.visualline": "block",
	"vim.foldfix": true,
	"vim.hlsearch": true,
	"vim.highlightedyank.enable": true,
	"vim.leader": "<space>",
	"vim.normalModeKeyBindings": [
		{
			"before": ["[", "d"],
			"commands": ["editor.action.marker.prev"]
		},
		{
			"before": ["]", "d"],
			"commands": ["editor.action.marker.next"]
		},
		{
			"before": ["<leader>", "/",],
			"commands": ["actions.find"]
		},
		{
			"before": ["<leader>", "c", "a"],
			"commands": ["editor.action.quickFix"]
		},
		{
			"before": ["<leader>", "f"],
			"commands": ["workbench.action.quickOpen"]
		},
		{
			"before": ["<leader>", "f", "d"],
			"commands": ["editor.action.formatDocument"]
		},
		{
			"before": ["<leader>", "t"],
			"commands": ["workbench.action.focusAuxiliaryBar"]
		},
		{
			"before": ["<leader>", "v", "r", "n"],
			"commands": ["editor.action.rename"]
		},
		{
			"before": ["g", "d"],
			"commands": ["editor.action.peekDefinition"]
		},
		{
			"before": ["g", "r"],
			"commands": ["editor.action.referenceSearch.trigger"]
		},
		{
			"before": ["K"],
			"commands": ["editor.action.showHover"]
		},
	],
	"vim.visualModeKeyBindings": [
		{
			"before": ["J"],
			"commands": ["editor.action.moveLinesDownAction"]
		},
		{
			"before": ["K"],
			"commands": ["editor.action.moveLinesUpAction"]
		}
	],
	"workbench.colorCustomizations": {
		"editorError.foreground": "#FFFFFF50",
		"editorInfo.foreground": "#FFFFFF50",
		"editorWarning.foreground": "#FFFFFF50"
	},
	"workbench.colorTheme": "One Dark Pro Mix",
	"workbench.editor.empty.hint": "hidden",
	"workbench.iconTheme": "material-icon-theme",
	"workbench.list.mouseWheelScrollSensitivity": 0.5,
	"[python]": {
		"editor.defaultFormatter": "ms-python.black-formatter",
		"editor.tabSize": 4
	},
	"workbench.settings.useSplitJSON": true
}